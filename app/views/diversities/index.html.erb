<p id="notice"><%= notice %></p>

<h1>Diversities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diversities.each do |diversity| %>
      <tr>
        <td><%= link_to 'Show', diversity %></td>
        <td><%= link_to 'Edit', edit_diversity_path(diversity) %></td>
        <td><%= link_to 'Destroy', diversity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diversity', new_diversity_path %>
